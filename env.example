# Quirkly Extension Environment Configuration
# Copy this file to env.js and update the values for your environment

# Environment (development or production)
NODE_ENV=development

# Base URL for n8n workflows
N8N_BASE_URL=https://ai.technioz.com

# Development endpoints (used when NODE_ENV=development)
DEV_AUTH_ENDPOINT=/webhook-test/quirkly-auth
DEV_REPLY_ENDPOINT=/webhook-test/replyai-webhook
DEV_DASHBOARD_URL=http://localhost:3000

# Production endpoints (used when NODE_ENV=production)
PROD_AUTH_ENDPOINT=/webhook/quirkly-auth
PROD_REPLY_ENDPOINT=/webhook/replyai-webhook
PROD_DASHBOARD_URL=https://quirkly.technioz.com

# Additional configuration
DEBUG=true
LOG_LEVEL=info
